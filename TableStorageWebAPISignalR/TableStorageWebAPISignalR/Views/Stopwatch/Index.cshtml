@Scripts.Render("~/bundles/App")
<html ng-app="appmodule">
<head>
</head>
<body ng-controller="stopwatchcontroller">
    <h1>StopWatch Information</h1>
    <h2>
        <span>{{userName}}</span>
        <input type="button" class="btn" value="logoff" ng-click="logout()" />
    </h2>

    <table class="table table-bordered table-striped table-condensed">
        <tr>
            <td>Stop-Watch Name:</td>
            <td>
                <input type="text" class="form-control"
                       ng-model="UserName" />
            </td>
        </tr>      
        <tr>
            <td></td>
            <td></td>
        </tr>
        <tr>
            <td></td>
            <td>
                <input type="button" value="Load" class="form-control btn btn-success"
                       ng-click="loadStopwatch()" />
                
            </td>
            <td>
                <input type="button" value="Start/Stop Stop-watch" class="form-control btn btn-success"
                       ng-click="StartStopStopwatch()" />
                
            </td>
        </tr>
    </table>
    <div><span>{{Message}}</span></div>

    <div><span>{{stopwatch}}</span></div>
